{
  "name": "@abide-community/components",
  "version": "0.0.0-development",
  "description": "React components used by the abide.community sites",
  "license": "MIT",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "parcel docs/index.html -d public -p 4000",
    "build": "NODE_ENV=production rollup -c",
    "postbuild": "bundlesize -f 'dist/*.js'",
    "docs": "parcel build docs/index.html -d public",
    "test": "jest"
  },
  "peerDependencies": {
    "react": ">=16.7.0-alpha.0"
  },
  "dependencies": {
    "@mdx-js/tag": "^0.16.0",
    "@rebass/components": "^4.0.0-1",
    "rebass": "^2.3.4",
    "styled-components": "^4.0.3",
    "styled-system": "^3.1.11"
  },
  "devDependencies": {
    "@abide-community/parcel-plugin-clean-out-dir": "^1.0.0",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@mdx-js/parcel-plugin-mdx": "^0.16.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.19",
    "bundlesize": "^0.17.0",
    "eslint": "^5.8.0",
    "eslint-config-macklinu": "^1.1.1",
    "husky": "^1.1.3",
    "jest": "^23.6.0",
    "jest-prop-type-error": "^1.1.0",
    "jest-styled-components": "^6.2.2",
    "lint-staged": "^8.0.4",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.15.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-testing-library": "^5.2.3",
    "regenerator-runtime": "^0.12.1",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,mdx}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./jest.setup.js",
    "setupFiles": [
      "jest-prop-type-error"
    ]
  },
  "alias": {
    "@abide-community/components": "./src"
  }
}
